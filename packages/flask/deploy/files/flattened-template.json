{
  "template" : "monitoring*",
  "order" : 10,
  "mappings": {
    "_default_" : {
      "dynamic_templates" : [ {
        "date_fields" : {
          "mapping" : {
            "type" : "date",
            "doc_values": true,
            "format" : "dateOptionalTime"
          },
          "match_mapping_type" : "date",
          "match" : "*"
        }
      },
      {
        "byte_fields" : {
          "mapping" : {
            "type" : "byte",
            "doc_values": true
          },
          "match_mapping_type" : "byte",
          "match" : "*"
        }
      },
      {
        "double_fields" : {
          "mapping" : {
            "type" : "double",
            "doc_values": true
          },
          "match_mapping_type" : "double",
          "match" : "*"
        }
      },
      {
        "float_fields" : {
          "mapping" : {
            "type" : "float",
            "doc_values": true
          },
          "match_mapping_type" : "float",
          "match" : "*"
        }
      },
      {
        "integer_fields" : {
          "mapping" : {
            "type" : "integer",
            "doc_values": true
          },
          "match_mapping_type" : "integer",
          "match" : "*"
        }
      },
      {
        "long_fields" : {
          "mapping" : {
            "type" : "long",
            "doc_values": true
          },
          "match_mapping_type" : "long",
          "match" : "*"
        }
      },
      {
        "short_fields" : {
          "mapping" : {
            "type" : "short",
            "doc_values": true
          },
          "match_mapping_type" : "short",
          "match" : "*"
        }
      },
      {
        "string_fields" : {
          "mapping" : {
            "index" : "not_analyzed",
            "omit_norms" : true,
            "type" : "string",
            "doc_values": true
          },
          "match_mapping_type" : "string",
          "match" : "*"
        }
      }
      ],
      "_all" : {
        "enabled" : true
      },
      "properties" : {
        "@version" : {
          "index" : "not_analyzed",
          "type" : "string",
          "doc_values": true
        }
      }
    },
    "metric": {
      "dynamic_templates": [],
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "environment": {
            "type": "string",
            "index": "not_analyzed"
        },
        "host": {
            "type": "string",
            "index": "not_analyzed"
        },
        "kind": {
            "type": "string",
            "index": "not_analyzed"
        },
        "name": {
            "type": "string",
            "index": "not_analyzed"
        },
        "cluster": {
            "type": "string",
            "index": "not_analyzed"
        },
        "stack": {
            "type": "string",
            "index": "not_analyzed"
        },
        "units": {
            "type": "string",
            "index": "not_analyzed"
        },
        "uri": {
            "type": "string",
            "index": "not_analyzed"
        },
        "window": {
            "type": "string",
            "index": "not_analyzed"
        },
        "destination": {
            "type": "string",
            "index": "not_analyzed"
        },
        "edge": {
            "type": "string",
            "index": "not_analyzed"
        },
        "experiment_id": {
            "type": "string",
            "index": "not_analyzed"
        },
        "experiment_group": {
            "type": "string",
            "index": "not_analyzed"
        }
      }
    }
  }
}
