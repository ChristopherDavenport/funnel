{
    "order": 10,
    "template": "monitoring*",
    "mappings": {
        "_default_": {
            "_all": {
                "enabled": true
            },
            "dynamic_templates": [
                {
                    "date_fields": {
                        "mapping": {
                            "doc_values": true,
                            "format": "dateOptionalTime",
                            "type": "date"
                        },
                        "match": "*",
                        "match_mapping_type": "date"
                    }
                },
                {
                    "byte_fields": {
                        "mapping": {
                            "doc_values": true,
                            "type": "byte"
                        },
                        "match": "*",
                        "match_mapping_type": "byte"
                    }
                },
                {
                    "double_fields": {
                        "mapping": {
                            "doc_values": true,
                            "type": "double"
                        },
                        "match": "*",
                        "match_mapping_type": "double"
                    }
                },
                {
                    "float_fields": {
                        "mapping": {
                            "doc_values": true,
                            "type": "float"
                        },
                        "match": "*",
                        "match_mapping_type": "float"
                    }
                },
                {
                    "integer_fields": {
                        "mapping": {
                            "doc_values": true,
                            "type": "integer"
                        },
                        "match": "*",
                        "match_mapping_type": "integer"
                    }
                },
                {
                    "long_fields": {
                        "mapping": {
                            "doc_values": true,
                            "type": "long"
                        },
                        "match": "*",
                        "match_mapping_type": "long"
                    }
                },
                {
                    "short_fields": {
                        "mapping": {
                            "doc_values": true,
                            "type": "short"
                        },
                        "match": "*",
                        "match_mapping_type": "short"
                    }
                },
                {
                    "string_fields": {
                        "mapping": {
                            "doc_values": true,
                            "index": "not_analyzed",
                            "omit_norms": true,
                            "type": "string"
                        },
                        "match": "*",
                        "match_mapping_type": "string"
                    }
                }
            ],
            "properties": {
                "@version": {
                    "doc_values": true,
                    "index": "not_analyzed",
                    "type": "string"
                }
            }
        },
        "metric": {
            "dynamic_templates": [],
            "properties": {
                "@timestamp": {
                    "format": "dateOptionalTime",
                    "type": "date"
                },
                "cluster": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "counter": {
                    "type": "integer"
                },
                "edge": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "environment": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "experiment_group": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "experiment_id": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "guage.double": {
                    "type": "double"
                },
                "guage.stats": {
                    "properties": {
                        "count": {
                            "type": "integer"
                        },
                        "kurtosis": {
                            "type": "double"
                        },
                        "last": {
                            "type": "double"
                        },
                        "mean": {
                            "type": "double"
                        },
                        "skewness": {
                            "type": "double"
                        },
                        "standardDeviation": {
                            "type": "double"
                        },
                        "variance": {
                            "type": "double"
                        }
                    }
                },
                "guage.string": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "host": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "kind": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "name": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "numeric": {
                    "properties": {
                        "count": {
                            "type": "integer"
                        },
                        "kurtosis": {
                            "type": "double"
                        },
                        "last": {
                            "type": "double"
                        },
                        "mean": {
                            "type": "double"
                        },
                        "skewness": {
                            "type": "double"
                        },
                        "standardDeviation": {
                            "type": "double"
                        },
                        "variance": {
                            "type": "double"
                        }
                    }
                },
                "stack": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "timer": {
                    "properties": {
                        "count": {
                            "type": "integer"
                        },
                        "kurtosis": {
                            "type": "double"
                        },
                        "last": {
                            "type": "double"
                        },
                        "mean": {
                            "type": "double"
                        },
                        "skewness": {
                            "type": "double"
                        },
                        "standardDeviation": {
                            "type": "double"
                        },
                        "variance": {
                            "type": "double"
                        }
                    }
                },
                "traffic": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "type": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "units": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "uri": {
                    "index": "not_analyzed",
                    "type": "string"
                },
                "window": {
                    "index": "not_analyzed",
                    "type": "string"
                }
            }
        }
    }
}
